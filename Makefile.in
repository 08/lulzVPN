package        = @PACKAGE_NAME@
version        = @PACKAGE_VERSION@
srcdir         = @srcdir@
VPATH          = @srcdir@

all lulznet:
	$(MAKE) -C src $@

install: all
	cp lulznet /usr/bin/

uninstall:
	rm /usr/bin/lulznet

clean:
	$(MAKE) -C src $@

Makefile: Makefile.in config.status
	./config.status $@

config.status: configure
	./config.status --recheck

